<Window x:Class="ChessPresenter.PractiseWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PractiseWindow" Height="180" Width="450" Closed="Window_Closed">
    <Grid>
        <Label Content="Practise Time" Height="28" HorizontalAlignment="Left" Margin="70,10,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="162,12,0,0" Name="textBox1" VerticalAlignment="Top" Width="108" PreviewTextInput="textBox1_PreviewTextInput" />
        <TextBox Height="85" HorizontalAlignment="Left" Margin="12,46,0,0" Name="textBox2" VerticalAlignment="Top" Width="404" AcceptsReturn="True" IsReadOnly="True" />
        <Button Content="开始训练" Height="23" HorizontalAlignment="Left" Margin="288,12,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
    </Grid>
</Window>
