<Window x:Class="ChessPresenter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" Height="353" Width="246">
    <Grid Width="200" Height="305">
        <Button Content="新建AI" Height="23" HorizontalAlignment="Left" Name="NewAI" VerticalAlignment="Top" Width="75" Click="NewAI_Click" />
        <Button Content="删除AI" Height="23" HorizontalAlignment="Right" Margin="0,0,29,0" Name="DeleteAI" VerticalAlignment="Top" Width="75" Click="DeleteAI_Click" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="51,76,0,0" Name="WhiteComboBox" VerticalAlignment="Top" Width="120" Text="" />
        <Label Content="白棋AI" Height="28" HorizontalAlignment="Left" Margin="0,73,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="黑棋AI" Height="28" HorizontalAlignment="Left" Margin="0,129,0,0" Name="label2" VerticalAlignment="Top" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="51,129,0,0" Name="BlackComboBox" VerticalAlignment="Top" Width="120" />
        <Button Content="开始游戏" Height="34" HorizontalAlignment="Left" Margin="96,244,0,0" Name="GameStart" VerticalAlignment="Top" Width="75" Click="GameStart_Click" />
    </Grid>
</Window>
